{
  "configurePresets": [
    {
      "binaryDir": "build",
      "cacheVariables": {
        "CMAKE_INTERPROCEDURAL_OPTIMIZATION": {
          "type": "BOOL",
          "value": "ON"
        },
        "CMAKE_INTERPROCEDURAL_OPTIMIZATION_DEBUG": {
          "type": "BOOL",
          "value": "OFF"
        }
      },
      "errors": {
        "deprecated": true
      },
      "hidden": true,
      "name": "cmake-dev",
      "warnings": {
        "deprecated": true,
        "dev": true
      }
    },
	{
      "environment": {
        "CXXFLAGS": "/MP /W4 /WX /external:anglebrackets /external:W0 $penv{CXXFLAGS}"
      },
      "generator": "Visual Studio 17 2022",
      "hidden": true,
      "name": "vs2022"
    },
    {
      "cacheVariables": {
        "CMAKE_TOOLCHAIN_FILE": {
          "type": "STRING",
          "value": "$env{VCPKG_ROOT}/scripts/buildsystems/vcpkg.cmake"
        },
        "SKSE_SUPPORT_XBYAK": "ON",
        "ENABLE_SKYRIM_AE": "ON",
        "ENABLE_SKYRIM_SE": "OFF",
        "ENABLE_SKYRIM_VR": "OFF"
      },
      "hidden": true,
      "name": "vcpkg"
    },
    {
      "cacheVariables": {
        "Boost_USE_STATIC_LIBS": {
          "type": "BOOL",
          "value": "ON"
        },
        "Boost_USE_STATIC_RUNTIME": {
          "type": "BOOL",
          "value": "OFF"
        },
        "CMAKE_MSVC_RUNTIME_LIBRARY": {
          "type": "STRING",
          "value": "MultiThreaded$<$<CONFIG:Debug>:Debug>DLL"
        },
        "VCPKG_OVERLAY_PORTS": {
          "type": "STRING",
          "value": "${sourceDir}/cmake/ports"
        },
        "VCPKG_OVERLAY_TRIPLETS": {
          "type": "STRING",
          "value": "${sourceDir}/cmake/triplets"
        },
        "VCPKG_TARGET_TRIPLET": {
          "type": "STRING",
          "value": "x64-windows-custom"
        }
      },
      "hidden": true,
      "name": "windows"
    },
	{
      "description": "VS2022/Windows/VCPkg",
      "inherits": [
        "cmake-dev",
        "vcpkg",
        "vs2022",
        "windows"
      ],
      "name": "vs2022-windows-vcpkg"
    }
  ],
  "buildPresets": [
      {
          "name": "Debug",
          "description": "Debug",
          "displayName": "Debug",
          "configurePreset": "vs2022-windows-vcpkg",
          "configuration": "Debug"
      },
      {
          "name": "Release",
          "description": "Release",
          "displayName": "Release",
          "configurePreset": "vs2022-windows-vcpkg",
          "configuration": "Release"
      }
  ],
  "version": 2
}
